railer: data.videos }))\n\t\t.catch(error => console.log(error))\n\t}\n\n\tgoHome = () => {\n\t\tthis.setState({ isMovieDetails: false, movie: null, statusCode: 0})\n\t}\n\n\trender() {\n\t\treturn (\n\t\t\t<main className=\"App\">\n\t\t\t\t<header>\n\t\t\t\t\t<h1>Rancid Tomatillos</h1>\n\t\t\t\t\n\t\t\t\t\t<nav>\n\t\t\t\t\t\t<NavLink to='/movies'>\n\t\t\t\t\t\t{this.state.isMovieDetails && <button onClick={() => {this.goHome()}}>Back</button>}\n\t\t\t\t\t\t</NavLink>\n\t\t\t\t\t</nav>\n\t\t\t\t</header>\n\t\t\t\t\t<Route \n\t\t\t\t\t\texact \n\t\t\t\t\t\tpath='/movies' \n\t\t\t\t\t\trender={() => {\n\t\t\t\t\t\t\treturn (\n\t\t\t\t\t\t\t<MovieContainer \n\t\t\t\t\t\t\t\tmovies={this.state.movies} \n\t\t\t\t\t\t\t\tgetSingleMovie={this.getSingleMovie} \n\t\t\t\t\t\t\t\tstatusCode={this.state.statusCode}  \n\t\t\t\t\t\t\t\terror={this.state.error} \n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t)\n\t\t\t\t\t\t}}\n\t\t\t\t\t/> \n\t\t\t\t\t<Route \n\t\t\t\t\t\texact\n\t\t\t\t\t\tpath='/movies/:id'\n\t\t\t\t\t\trender={() => {\n\t\t\t\t\t\t\tif (!this.state.movie) {\n\t\t\t\t\t\t\t\treturn(\n\t\t\t\t\t\t\t\t\t<h1>Whoops, it looks like something went wrong!</h1>\n\t\t\t\t\t\t\t\t)\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\treturn (\t\n\t\t\t\t\t\t\t\t<MovieDetails \n\t\t\t\t\t\t\t\tmovie={this.state.movie}\n\t\t\t\t\t\t\t\tstatusCode={this.state.statusCode}  \n\t\t\t\t\t\t\t\terror={this.state.error} \n\t\t\t\t\t\t\t\tmovieTrailer={this.state.movieTrailer[0]}\n\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t)\n\t\t\t\t\t\t}}\n\t\t\t\t\t/>\n\t\t\t\t\n\t\t\t</main>\n\t\t);\n\t}\n}\n\nexport default App;\n","/Users/karacaputo/turing/3module/projects/rancid-tomatillos/src/MovieContainer/MovieContainer.js",[],"/Users/karacaputo/turing/3module/projects/rancid-tomatillos/src/Header/Header.js",[],"/Users/karacaputo/turing/3module/projects/rancid-tomatillos/src/apiCalls.js",[],"/Users/karacaputo/turing/3module/projects/rancid-tomatillos/src/MovieDetails/MovieDetails.js",["41","42","43"],"/Users/karacaputo/turing/3modu